          <TableColumn fx:id="tabName" prefWidth="150.0" text="Nazwa" />
          <TableColumn fx:id="tabType" prefWidth="150.0" text="Rodzaj" />
          <TableColumn fx:id="tabQuantity" prefWidth="82.0" text="Ilosc" />
